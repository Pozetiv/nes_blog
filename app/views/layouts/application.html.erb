<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <header>
      <%= render('shared/header') %>
    </header>
    <div class="container-fluid">
      <%= yield %>
    </div>

    <footer>
      <%= render 'shared/footer' %>
    </footer>
  </body>
</html>
